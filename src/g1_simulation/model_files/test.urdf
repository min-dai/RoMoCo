<robot name="test">

  <link name="world"/>

  <joint name="joint0" type="revolute">
    <parent link="world"/>
    <child link="link1"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-10" upper="10" effort="10" velocity="1"/>
  </joint>


  <link name="link1">
    <inertial>
      <mass value="1.0" />
      <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1" />
      <origin xyz="0.5 0 0" rpy="0 0 0" />
    </inertial>
  </link>

  <joint name="joint1" type="revolute">
    <parent link="link1" />
    <child link="link2" />
    <origin xyz="0.9 0 0" rpy="0 0 0" />
    <axis xyz="0 0 1" />
    <limit lower="-10" upper="10" effort="10" velocity="1" />
  </joint>

  <link name="link2">
    <inertial>
      <mass value="1.0" />
      <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1" />
      <origin xyz="0.5 0 0" rpy="0 0 0" />
    </inertial>
  </link>

</robot>
