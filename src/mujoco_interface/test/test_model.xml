<mujoco model="test_model">
    <compiler angle="degree" coordinate="local" />
    <option timestep="0.01" />

    <default>
        <joint limited="true" damping="1"/>
        <geom friction="1 0.5 0.5" />
    </default>

    <worldbody>
        <body name="base" pos="0 0 0.1">
            <joint name="joint_1" type="hinge" range="-1 1" axis="0 1 0" pos="0 0 0" />
            <joint name="joint_2" type="hinge" range="-1 1" axis="1 0 0" pos="0 0 0.1" />
            <geom name="base_geom" type="box" size="0.1 0.1 0.1" />
        </body>
    </worldbody>

    <actuator>
        <motor name="actuator_1" joint="joint_1" ctrlrange="-1 1" />
        <motor name="actuator_2" joint="joint_2" ctrlrange="-1 1" />
    </actuator>

    <sensor>
        <jointpos name="sensor_joint_1" joint="joint_1" />
        <jointvel name="sensor_joint_2" joint="joint_2" />
    </sensor>
</mujoco>
